{
  "name": "preline-react",
  "version": "1.1.0",
  "type": "module",
  "description": "React components library for Preline UI",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "prepare": "husky",
    "prepublishOnly": "pnpm run test",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "format": "prettier --write ."
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "preline",
    "tailwind",
    "tailwindcss",
    "typescript"
  ],
  "author": "minagishl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/minagishl/preline-react.git"
  },
  "bugs": {
    "url": "https://github.com/minagishl/preline-react/issues"
  },
  "homepage": "https://github.com/minagishl/preline-react#readme",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.14.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "lucide-react": "^0.544.0",
    "preline": "^3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/addon-links": "^9.1.0",
    "@storybook/react-vite": "^9.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "9.0.6",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rollup": "^4.46.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook": "^9.1.0",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
  }
}
